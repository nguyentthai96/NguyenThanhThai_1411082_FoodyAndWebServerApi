ing":8.600000,"UserID":8678520,"Username":"dyvnl500","Email":null,"FullName":"Ngo Lan","AvatarURL":"https://media.foody.vn/usr/g868/8678520/avt/c40x40/foody-avatar-898-636276068766039633.jpg","Gender":"F","IsVerified":false,"UserIsVerified":false,"OwnerFirstName":"Ngo","OwnerLastName":"Lan"},{"ReviewID":1328003,"RestaurantID":4460,"Title":"Qu\u0026#225;n trang tr\u0026#237; cổ điển","Comment":"Mình đến đây 2 lần, 1 lần uống cocktail và lần thứ 2 ăn tối. Mình order beefsteak bảo làm midium nhưng thịt vẫn rất đỏ, phải gửi đi nướng lại lần 2. Món ăn bình thường.\nCocktail ok, giá hợp lí.","CreatedOn":"\/Date(1490267306567)\/","AvgRating":7.000000,"UserID":84325,"Username":"myduyenbt1903","Email":null,"FullName":"Mỹ Duyên Phan","AvatarURL":"https://media.foody.vn/default/s40/user-default-male.png","Gender":null,"IsVerified":false,"UserIsVerified":false,"OwnerFirstName":"Mỹ Duyên","OwnerLastName":"Phan"}],"OperatingView":{"Code":"1","Content":"Opening"},"IsAdvertise":null,"LocationUrlRewriteName":"ho-chi-minh","IsServiceDelivery":false,"IsServiceBooking":false,"IsServiceECard":false,"Services":[],"MemberCardStats":null,"PhotoUrl":"https://media.foody.vn/res/g1/4460/prof/s/foody-mobile-4p898ymz-jpg-181-635933035926641885.jpg","Has7UpEvent":false,"EventUrl":null},{"RestaurantID":1862,"Name":"Tám Phước - Quán Gà Vườn ","Name_EN":null,"Address":"8 Đường Số 12, Trần Não, P. Bình An, Quận 2, TP. HCM","DistrictID":0,"Latitude":0,"Longtitude":0,"CreatedOn":"\/Date(-62135596800000)\/","MetaTitle":null,"UrlRewriteName":"tam-phuoc-quan-ga-vuon","Phone":null,"MobileImageUrl":"foody-mobile-ga-vuon-tam-phuoc-mb-232-635741918846374614.jpg","TotalReviews":33,"TotalPictures":174,"TotalViews":0,"AvgRating":7.190000,"FeaturePicture":[{"AlbumID":null,"UserID":null,"RestaurantID":null,"PhotoUrl":"https://media.foody.vn/res/g1/1862/s180x180/foody-album44-jpg-248-635951188499778697.jpg","PictureID":0,"DisplayOrder":0,"CreatedOn":"\/Date(1459496850133)\/","PostedByDevice":null,"ReviewID":null,"
strictID":0,"Latitude":0,"Longtitude":0,"CreatedOn":"\/Date(-62135596800000)\/","MetaTitle":null,"UrlRewriteName":"tinh-tra-quan-207706","Phone":null,"MobileImageUrl":"foody-mobile-155082_3791300955162-348-635894805765340559.jpg","TotalReviews":2,"TotalPictures":4,"TotalViews":0,"AvgRating":6.300000,"FeaturePicture":[],"TopReViews":[{"ReviewID":534859,"RestaurantID":207706,"Title":"Tịnh Tr\u0026#224; Qu\u0026#225;n","Comment":"Không gian đẹp, yên tĩnh, hòa hợp với thiên nhiên. Món trà cũng vừa túi tiền. Nói chung là cũng tạm ổn.","CreatedOn":"\/Date(1454333134757)\/","AvgRating":5.000000,"UserID":1464558,"Username":"giangphamhuong211","Email":null,"FullName":"Phạm Giang","AvatarURL":"https://media.foody.vn/usr/g147/1464558/avt/c40x40/foody-avatar-300-636250321439718158.jpg","Gender":"","IsVerified":false,"UserIsVerified":false,"OwnerFirstName":"Phạm Giang","OwnerLastName":""},{"ReviewID":526936,"RestaurantID":207706,"Title":"Tịnh Tr\u0026#224; Qu\u0026#225;n","Comment":"Quán khá đẹp, yên tĩnh. Có cả không gian sân vườn ngoài trời và không gian kín máy lạnh. Phục vụ vui vẻ nhiệt tình. Có chỗ giữ xe miễn phí. Thức uống thì bình thường không có gì đặc biệt.","CreatedOn":"\/Date(1453647191947)\/","AvgRating":7.600000,"UserID":359947,"Username":"tieutu12mov","Email":null,"FullName":"Lâm Hữu","AvatarURL":"https://media.foody.vn/usr/g36/359947/avt/c40x40/tieutu12mov-avatar-286-636209515811982825.jpg","Gender":"F","IsVerified":true,"UserIsVerified":false,"OwnerFirstName":"Lâm","OwnerLastName":"Hữu"}],"OperatingView":{"Code":"2","Content":"Closed"},"IsAdvertise":null,"LocationUrlRewriteName":"ho-chi-minh","IsServiceDelivery":false,"IsServiceBooking":false,"IsServiceECard":false,"Services":[],"MemberCardStats":null,"PhotoUrl":"https://media.foody.vn/res/g21/207706/prof/s/foody-mobile-155082_3791300955162-348-635894805765340559.jpg","Has7UpEvent":false,"EventUrl":null},{"RestaurantID":124529,"Name":"Hy Vọng Cafe - Đường Số 9","Name_EN":null,"Address":"86/4 Đường Số 9,P. Linh Tây, Quận Thủ Đức, TP. HCM","DistrictID":0,"Latitude":0,"Longtitude":0,"CreatedOn":"\/Date(-62135596800000)\/","MetaTitle":null,"UrlRewriteName":"hy-vong-cafe-duong-so-9","Phone":null,"MobileImageUrl":"foody-mobile-t-2-jpg-391-635614893545877582.jpg","TotalReviews":2,"TotalPictures":2,"TotalViews":0,"AvgRating":6.600000,"FeaturePicture":[],"TopReViews":[{"ReviewID":1161150,"RestaurantID":124529,"Title":"Hy Vọng Cafe - Đường Số 9","Comment":"Quán này chủ là Anh Hà.\nmột trong những anh em của Anh Sáu Phi\nkhông gian khá lý tưởng ở thuở ban sơ, nhưng vì thay đổi diện mạo nên quán đã mất đi những gì đang có. hiện tại quán vắng khách. \nlàm khách quán này thì rất tuyệt, \ntrước có hát hò hằng đêm nhưng giờ đã dẹp bỏ","CreatedOn":"\/Date(1480672042643)\/","AvgRating":6.200000,"UserID":8176270,"Username":"hoangnghiamanh.trungtin","Email":null,"FullName":"ĐIện Lạnh Trung Tín","AvatarURL":"https://media.foody.vn/usr/g818/8176270/avt/c40x40/foody-avatar-288-636162936618310269.jpg","Gender":"F","IsVerified":false,"UserIsVerified":false,"OwnerFirstName":"ĐIện Lạnh","OwnerLastName":"Trung Tín"},{"ReviewID":456886,"RestaurantID":124529,"Title":"Qu\u0026#225;n tạm ổn","Comment":"không gian quán rộng rãi, thoải mái, sạch sẽ. Không thích hợp ngồi buổi sáng, minh chứng là ít khách buổi sáng. Giá nước cũng tạm ổn. Nhân viên nhiệt tình.","CreatedOn":"\/Date(1448154319280)\/","AvgRating":7.000000,"UserID":630660,"Username":"xuanhao0101","Email":null,"FullName":"Hào Đặng","AvatarURL":"https://media.foody.vn/usr/g64/630660/avt/c40x40/foody-avatar-276-636153549274007970.jpg","Gender":"","IsVerified":false,"UserIsVerified":false,"OwnerFirstName":"Hào Đặng","OwnerLastName":""}],"OperatingView":{"Code":"2","Content":"Closed"},"IsAdvertise":null,"LocationUrlRewriteName":"ho-chi-minh","IsServiceDelivery":false,"IsServiceBooking":false,"IsServiceECard":false,"Services":[],"MemberCardStats":null,"PhotoUrl":"https://media.foody.vn/res/g13/124529/prof/s/foody-mobile-t-2-jpg-391-635614893545877582.jpg","Has7UpEvent":false,"EventUrl":null},{"RestaurantID":142093,"Name":"Sinh Tố Bà Chửi - Làng Đại Học","Name_EN":null,"Address":"Làng Đại Học, P. Linh Trung, Quận Thủ Đức, TP. HCM","DistrictID":0,"Latitude":0,"Longtitude":0,"CreatedOn":"\/Date(-62135596800000)\/","MetaTitle":null,"UrlRewriteName":"sinh-to-ba-chui-lang-dai-hoc","Phone":null,"MobileImageUrl":"foody-mobile-t1-jpg-628-635788009187915357.jpg","TotalReviews":6,"TotalPictures":3,"TotalViews":0,"AvgRating":4.766000,"FeaturePicture":[],"TopReViews":[{"ReviewID":1155178,"RestaurantID":142093,"Title":"Kh\u0026#244;ng c\u0026#243; g\u0026#236; đặc sắc","Comment":"Mình ghé được một lần rồi không ghé lại nữa, không phải vì bị chửi hay do không hài lòng về dịch vụ gì mà không quay lại chỉ đơn giản là thấy không có gì đặc biệt cả. Quán bán nước ngọt và sinh tố, mình thấy sinh tố cũng được, tạm. Còn nước ngọt thì trong chai thuỷ tinh ấy. Thấy không đảm bảo. Hôm ấy nhóm mình đi cũng đông, cũng nhốn nhắng nhưng chẳng bị chửi gì cả. Từ đó tới giờ chưa quay lại lần nào vì mình thấy trong khu làng đại học còn nhiều quán sinh tố ngon sạch rẻ lắm.","CreatedOn":"\/Date(1480407171337)\/","AvgRating":6.000000,"UserID":1820047,"Username":"p3_can15","Email":null,"FullName":"@trakity 1518","AvatarURL":"https://media.foody.vn/usr/g183/1820047/avt/c40x40/p3_can15-avatar-353-636286678208127213.jpg","Gender":"F","IsVerified":true,"UserIsVerified":false,"OwnerFirstName":"@trakity","OwnerLastName":"1518"},{"ReviewID":877931,"RestaurantID":142093,"Title":"Sinh Tố B\u0026#224; Chửi - L\u0026#224;ng Đại Học","Comment":"Thiệt tình nhiều bạn không biết, cứ chê quán. Mình thấy sinh tố ly đầy nhóc, lại ngon, giá lại rẻ :D . Duyệt . Quán gần KTX khu A nha.","CreatedOn":"\/Date(1465131019257)\/","AvgRating":7.400000,"UserID":1631369,"Username":"tinhlt12404a","Email":null,"FullName":"Lương Thị Tịnh","AvatarURL":"https://media.foody.vn/defaultavatar/s40/default-avatar-20130809-30.jpg","Gender":"F","IsVerified":true,"UserIsVerified":false,"OwnerFirstName":"Lương Thị","OwnerLastName":"Tịnh"}],"OperatingView":{"Code":"2","Content":"Closed"},"IsAdvertise":null,"LocationUrlRewriteName":"ho-chi-minh","IsServiceDelivery":false,"IsServiceBooking":false,"IsServiceECard":false,"Services":[],"MemberCardStats":null,"PhotoUrl":"https://media.foody.vn/res/g15/142093/prof/s/foody-mobile-t1-jpg-628-635788009187915357.jpg","Has7UpEvent":false,"EventUrl":null},{"RestaurantID":93199,"Name":"Cỏ Bốn Lá Cafe - Làng Đại Học","Name_EN":null,"Address":"Làng Đại Học Quốc Gia, Quận Thủ Đức, TP. HCM","DistrictID":0,"Latitude":0,"Longtitude":0,"CreatedOn":"\/Date(-62135596800000)\/","MetaTitle":null,"UrlRewriteName":"co-bon-la-cafe-lang-dai-hoc","Phone":null,"MobileImageUrl":"foody-mobile-co-4-la-jpg-969-636135901794604874.jpg","TotalReviews":8,"TotalPictures":24,"TotalViews":0,"AvgRating":7.104000,"FeaturePicture":[],"TopReViews":[{"ReviewID":1150849,"RestaurantID":93199,"Title":"Nơi sinh vi\u0026#234;n (hay couple) tụ họp","Comment":"Mình ghé quán được vài lần do vô chơi ma sói với mấy đứa bạn. Quán có trệt và một lầu lên nhưng khá nhỏ, phía trên có máy lạnh. Đồ uống vài món đá xay thì khá ổn. Chỉ có hôm bữa bạn mình gọi chanh dây thì nhạt nhẽo không nghe tí mùi chanh dây nào nên phải mang cái ly xuống dưới kêu làm lại. Giá cũng không phải là rẻ, hơi nhỉnh hơn những quán khác trong khu vực này. \nCó lần thấy mấy couple vô hug kiss bla bla làm mình ngại nóng cả mặt 😂😂\nChắc không quay lại đâu :))","CreatedOn":"\/Date(1480215038283)\/","AvgRating":6.000000,"UserID":1820047,"Username":"p3_can15","Email":null,"FullName":"@trakity 1518","AvatarURL":"https://media.foody.vn/usr/g183/1820047/avt/c40x40/p3_can15-avatar-353-636286678208127213.jpg","Gender":"F","IsVerified":true,"UserIsVerified":false,"OwnerFirstName":"@trakity","OwnerLastName":"1518"},{"ReviewID":1109065,"RestaurantID":93199,"Title":"Vua y!","Comment":"Hôm trước học về rồi cùng mấy đứa bạn ghé vô quán giết thời gian. Quán này thì rất phù hợp để giết thời gian, tuy quán khá nhỏ nhưng quán vắng. Mình ngồi suốt 3 tiếng trong quán thì chỉ thấy khách vô chưa đến 5 lượt. \nĐồ uống ở quán thì cũng bình thường, nhưng như v thì cũng hợp lý vì giá rẻ chỉ trên dưới 20k 1 món. Mình uống chocolate bạc hà    ,vị nhạt, chỉ có hương của chocolate và hương bạc hà. Được 1 điều là chị chủ quán ở đây thì vui vẻ dễ thương, rất hay cười.\nNói chung quán này ko có gì đặc sắc, chỉ gọi là chấp nhận được.","CreatedOn":"\/Date(1477966116713)\/","AvgRating":6.800000,"UserID":2181034,"Username":"reyasa","Email":null,"FullName":"Ai Duy Ho","AvatarURL":"https://media.foody.vn/usr/g219/2181034/avt/c40x40/foody-avatar-981-636132935339766484.jpg","Gender":"","IsVerified":false,"UserIsVerified":false,"OwnerFirstName":"Ai Duy","OwnerLastName":"Ho"}],"OperatingView":{"Code":"2","Content":"Closed"},"IsAdvertise":null,"LocationUrlRewriteName":"ho-chi-minh","IsServiceDelivery":false,"IsServiceBooking":false,"IsServiceECard":false,"Services":[],"MemberCardStats":null,"PhotoUrl":"https://media.foody.vn/res/g10/93199/prof/s/foody-mobile-co-4-la-jpg-969-636135901794604874.jpg","Has7UpEvent":false,"EventUrl":null}],"lastId":93199,"TotalCount":175}
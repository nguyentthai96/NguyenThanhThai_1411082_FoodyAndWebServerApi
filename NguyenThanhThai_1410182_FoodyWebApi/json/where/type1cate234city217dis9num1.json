ỏ ra như vậy còn cả khối sự lựa chọn cho tiệc buffet hải sản - thậm chí rẻ và chất lượng hơn nhều.","CreatedOn":"\/Date(1493874383717)\/","AvgRating":3.400000,"UserID":689956,"Username":"thoadinh0203","Email":null,"FullName":"Thoa Dinh","AvatarURL":"https://media.foody.vn/usr/g69/689956/avt/c40x40/foody-avatar-288-636228910734128866.jpg","Gender":"F","IsVerified":false,"UserIsVerified":false,"OwnerFirstName":"Thoa","OwnerLastName":"Dinh"}],"OperatingView":{"Code":"2","Content":"Closed"},"IsAdvertise":null,"LocationUrlRewriteName":"ho-chi-minh","IsServiceDelivery":false,"IsServiceBooking":true,"IsServiceECard":false,"Services":[{"Id":1,"Url":"http://www.tablenow.vn/ho-chi-minh/cham-charm-buffet-sang-trong","Title":null,"Text":"Booking","Avatar":null,"IsOnly":0,"BackgroundColor":null,"RestaurantId":562}],"MemberCardStats":null,"PhotoUrl":"https://media.foody.vn/res/g1/562/prof/s/foody-mobile-109-jpg-361-635858632711885387.jpg","Has7UpEvent":false,"EventUrl":null},{"RestaurantID":4641,"Name":"ThaiExpress - Crescent Plaza","Name_EN":null,"Address":"105 Tôn Dật Tiên, P. Tân Phú, Quận 7, TP. HCM","DistrictID":0,"Latitude":0,"Longtitude":0,"CreatedOn":"\/Date(-62135596800000)\/","MetaTitle":null,"UrlRewriteName":"thaiexpress-crescent-mall","Phone":null,"MobileImageUrl":"20151124103743-640x400thaie1.jpg","TotalReviews":46,"TotalPictures":343,"TotalViews":0,"AvgRating":7.452000,"FeaturePicture":[{"AlbumID":null,"UserID":null,"RestaurantID":null,"PhotoUrl":"https://media.foody.vn/res/g1/4641/s180x180/foody-album133-jpg-211-635944943928720620.jpg","PictureID":0,"DisplayOrder":0,"CreatedOn":"\/Date(1458872393340)\/","PostedByDevice":null,"ReviewID":null,"Name":null,"PredefineAlbumID":null,"TotalLikes":0,"IsDraft":false,"TokenKey":null,"TotalComments":0,"OriginalWidth":null,"OriginalHeight":null,"IsExisted":null,"CheckinId":null,"IsLiked":false,"IsProPhoto":false,"BgColor":null,"UniqueId":null,"CreatedBy":null,"Status":0,"YouTubeCode":null,"VideoType":0,"FileName":null,"Type":nu